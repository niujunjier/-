/* @模态框样式 */
/* @zhangyun */
/* @2018-04-11 */
.modal{
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.2);
  position: absolute;
  top: 0;
  left: 0;
}
.modal-content{
  width: 562rpx;
  height: 598rpx;
  background: #fff;
  margin: 160rpx auto 0;
}
