/*单输入框弹出组件样式  */
.pretonext{
  width: 100%;
  height: 218rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.pretonext input{
  width: 161rpx;
  height: 71rpx;
  border: 1rpx solid #cdcdcd;
  text-align: center;
}
.pretonext .toclass{
  width: 161rpx;
  height: 71rpx;
  line-height: 71rpx;
  background-color: #ed6d00;
  color: #fff;
  text-align: center;
}

.msg-view{
  font-size: 28rpx;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
}
.primary-input{
  width: 500rpx;
  margin: 10rpx auto;
  font-size: 28rpx;
  height: 65rpx;
  line-height: 65rpx;
}
.button-input{
  width: 500rpx;
  margin: 0 auto;
  font-size: 28rpx;
}
.button-input input{
  display: inline-block;
  width: 343rpx;
  height: 65rpx;
  vertical-align: middle;
}
.btn-inline{
  vertical-align: middle;
  width: 143rpx;
  display: inline-block;
  font-size: 24rpx;
  height: 63rpx;
   line-height: 63rpx; 
  text-align: center;
  border: 4rpx solid #ececec;
  margin-left: 5rpx;
  border-radius: 10rpx;
}
.title-input{
  width: 500rpx;
  margin: 0 auto;
  font-size: 28rpx;
}
.title-input input{
  display: inline-block;
  width: 343rpx;
  height: 65rpx;
  vertical-align: middle;
}
.title-input .tview{
  display: inline-block;
  vertical-align: middle;
  width: 143rpx;
  height: 63rpx;
  line-height: 63rpx; 
  text-align: center;
}