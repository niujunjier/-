/**app.wxss**/
@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}

page{
  height: 100%;
  background-color: #e5e5e5;
}
.colorGrey{
  color: #cdcdcd;
}
.btn{
  width: 100%;
  height: 91rpx;
  line-height: 91rpx;
  text-align: center;
  font-size: 28rpx;
}
.btn-default{
  background-color: #ffffff;
}
.border-bottom{
  border-bottom: 1rpx solid #ececec;
}
.btn{
  display: flex;
  justify-content: center;
  align-items: center;
}
.btn-md{
  font-size: 28rpx;
  width: 364rpx;
  height: 73rpx;
  margin: 40rpx auto;
  color: #fff;
  border-radius: 10rpx;
} 
.btn-lg{
  font-size: 28rpx;
  width: 691rpx;
  height: 89rpx;
  margin: 40rpx auto;
  color: #fff;
  border-radius: 10rpx;
} 
.btn-success{
  background-color: #ed6d00;
}
view{
  box-sizing: border-box;
}

/*图标字体  */
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  color: #000;
}

