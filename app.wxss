/**app.wxss**/
page{
  height: 100%;
  background-color: #e5e5e5;
}
/* 每个页面盒子容器 */
.container{
  width: 100%;
  height: 100%;
  background: #fff;
}
.colorGrey{
  color: #cdcdcd;
}
.btn{
  width: 100%;
  height: 91rpx;
  line-height: 91rpx;
  text-align: center;
  font-size: 28rpx;
}
.btn-default{
  background-color: #ffffff;
}
/* 模态框样式 */
.modal{
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.2);
  position: absolute;
  top: 0;
  left: 0;
}
.modal-content{
  width: 562rpx;
  height: 598rpx;
  background: #fff;
  margin: 160rpx auto 0;
}
/* 字体大小 */
.font14{
  font-size: 14px;
}
.font12{
  font-size: 12px;
}
/* 字体颜色 */
.orange{
  color: rgb(237,109,0);
}
.gray{
  color: rgb(149, 149, 149)
}
.blue{
  color: rgb(86,212,246)
}
.yellow{
  color: rgb(242,193,130)
} 