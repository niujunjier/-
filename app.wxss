/**app.wxss**/
@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}

.orange{
  color: #ed6d00;
}
.blue{
  color: #56d4f6;
}

page{
  height: 100%;
  background-color: #e5e5e5;
}
/* 每个页面盒子容器 */
.container{
  width: 100%;
  height: 100%;
  background: #fff;
}
.colorGrey{
  color: #cdcdcd;
}
.btn-full{
  width: 100%;
  height: 91rpx;
  font-size: 28rpx;
}
.btn-default{
  background-color: #ffffff;
}
.border-bottom{
  border-bottom: 1rpx solid #ececec;
}
.btn{
  display: flex;
  justify-content: center;
  align-items: center;
}
.btn-md{
  font-size: 28rpx;
  width: 364rpx;
  height: 73rpx;
  margin: 40rpx auto;
  color: #fff;
  border-radius: 10rpx;
} 
.btn-lg{
  font-size: 28rpx;
  width: 691rpx;
  height: 89rpx;
  margin: 40rpx auto;
  color: #fff;
  border-radius: 10rpx;
} 
.btn-success{
  background-color: #ed6d00;
}
view{
  box-sizing: border-box;
}
.left{
  float: left;
}

.home-item-box{
  width: 611rpx;
  margin: 0 auto;
}
.home-item-title{
  font-size: 28rpx;
}
.home-item-content{
  width: 611rpx;
  height: 201rpx;
  margin-top: 10rpx;
}
.or-shadow{
  box-shadow: 0 10rpx 15px #ed6d00;
}
.bl-shadow{
  box-shadow: 0 10rpx 15px #56d4f6;
}

.font12{
  font-size: 12rpx;
}
.font14{
  font-size: 14rpx;
}
.font28{
  font-size: 28rpx;
}


