@import '../../app.wxss';

.mask-body{
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.1);
  position: absolute;
  top: 0;
  left: 0;
}
.mask-main{
  width: 562rpx;
  min-height: 200rpx;
  margin: 200rpx auto;
  background-color: #fff;
  border-radius: 10rpx;
  padding-bottom: 40rpx;
}
.title{
  width: 100%;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  font-size: 28rpx;
}
.close{
  float: right;
  font-size: 30rpx;
   margin: -15rpx 15rpx 0 0; 
}